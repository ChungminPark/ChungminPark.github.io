# 설명
# Gemfile: 사이트에 필요한 gem들의 목록
# bundler: Gemfile에 있는 gem들을 설치하는 gem

# 실행 방법 - http://labs.brandi.co.kr/2018/05/14/chunbs.html, https://jekyllrb-ko.github.io/
# gem install jekyll
# gem install bundler
# bundler install
# bundle exec jekyll serve

source "https://rubygems.org"

gem "github-pages", group: :jekyll_plugins
gem "jekyll-include-cache", group: :jekyll_plugins
gem "webrick", "~> 1.8"
